{
  "name": "ENAT",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-expo": "^5.2.0",
    "eslint": "4.19.1",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-prettier": "2.1.2",
    "eslint-plugin-react": "7.1.0",
    "eslint-plugin-react-native": "3.0.1",
    "flow-bin": "0.52.0",
    "flow-typed": "2.1.5",
    "husky": "0.14.3",
    "jest": "20.0.4",
    "jest-expo": "^34.0.1",
    "prettier": "1.5.3",
    "react-native-scripts": "1.11.1",
    "react-test-renderer": "16.5.2"
  },
  "main": "./node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "flow": "flow",
    "pretty": "prettier --semi false --print-width 100 --single-quote --trailing-comma all --write \"src/**/*.js\""
  },
  "lint-staged": {
    "*.js": [
      "npm pretty",
      "git add"
    ]
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|lottie-react-native|expo|react-native-maps|react-native-svg|react-native-branch|@shoutem/theme|@shoutem/animation|@shoutem/ui|native-base-shoutem-theme|react-native-easy-grid|react-native-drawer|react-native-vector-icons|react-native-keyboard-aware-scroll-view|react-navigation|native-base|@expo|react-native-scrollable-tab-view|react-native-iphone-x-helper)/)"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.history/",
      "/e2e/",
      "/src/theme/"
    ]
  },
  "dependencies": {
    "color": "1.0.3",
    "enzyme": "^3.7.0",
    "expo": "^34.0.1",
    "expo-asset": "^6.0.0",
    "expo-av": "^6.0.0",
    "expo-camera": "^6.0.0",
    "expo-constants": "^6.0.0",
    "expo-file-system": "^6.0.2",
    "expo-font": "^6.0.1",
    "expo-permissions": "^6.0.0",
    "expokit": "^34.0.2",
    "lodash": "4.13.1",
    "moment": "^2.13.0",
    "native-base": "^2.13.5",
    "react": "16.8.3",
    "react-dom": "^16.5.2",
    "react-mixin": "^5.0.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-34.0.1.tar.gz",
    "react-native-easy-grid": "^0.2.0",
    "react-native-phone-call": "^1.0.9",
    "react-native-slider": "^0.11.0",
    "react-native-unimodules": "^0.5.2",
    "react-navigation": "1.5.0",
    "react-timer-mixin": "^0.13.4",
    "whatwg-fetch": "^2.0.4"
  }
}
